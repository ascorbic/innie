{
  "name": "@innie-ai/calendar",
  "version": "1.0.0",
  "description": "Calendar MCP server using AppleScript to query Calendar.app",
  "type": "module",
  "main": "dist/index.mjs",
  "bin": "dist/index.mjs",
  "files": ["dist"],
  "exports": {
    ".": "./dist/index.mjs"
  },
  "scripts": {
    "build": "tsdown src/*.ts --format esm --dts --clean",
    "dev": "tsdown src/*.ts --format esm --dts --watch",
    "start": "node dist/index.mjs",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "tsdown": "^0.18.4",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+github.com:ascorbic/innie.git",
    "directory": "packages/calendar"
  },
  "homepage": "https://github.com/ascorbic/innie",
  "author": "Matt Kane",
  "license": "MIT"
}
