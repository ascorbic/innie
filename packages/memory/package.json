{
  "name": "@innie/memory",
  "version": "1.0.0",
  "description": "Memory MCP server for semantic search and journaling",
  "type": "module",
  "main": "dist/index.js",
  "files": ["dist"],
  "exports": {
    ".": "./dist/index.js",
    "./embeddings": "./dist/embeddings.js",
    "./indexer": "./dist/indexer.js",
    "./journal": "./dist/journal.js"
  },
  "scripts": {
    "build": "tsdown src/*.ts --format esm --dts --clean",
    "dev": "tsdown src/*.ts --format esm --dts --watch",
    "prepublishOnly": "pnpm run build",
    "check": "publint && attw --pack --ignore-rules=cjs-resolves-to-esm",
    "test": "vitest"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "@xenova/transformers": "^2.17.2",
    "vectra": "^0.9.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "publint": "^0.3.16",
    "tsdown": "^0.18.4",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+github.com:ascorbic/innie.git",
    "directory": "packages/memory"
  },
  "homepage": "https://github.com/ascorbic/innie",
  "author": "Matt Kane",
  "license": "MIT"
}
